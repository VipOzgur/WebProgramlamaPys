<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<!-- NuGet paketlerini çekeceğimiz kaynak -->
		<clear />
		<!-- Önceden tanımlı kaynakları temizler -->
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
	</packageSources>

	<!-- Belirli bir paketin sürümünü zorlamak -->
	<packageVersionOverrides>
		<add package="Microsoft.VisualStudio.Web.CodeGeneration.Design" version="8.0.0" />
	</packageVersionOverrides>
</configuration>
